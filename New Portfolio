  <script>
    gsap.registerPlugin(ScrollTrigger);

    gsap.from("nav", { y: -25, opacity: 0, duration: 0.8, ease: "power3.out" });

    gsap.utils.toArray(".bento-card").forEach((card) => {
      gsap.fromTo(
        card,
        { opacity: 0, y: 26 },
        {
          opacity: 1,
          y: 0,
          duration: 0.9,
          ease: "power3.out",
          scrollTrigger: {
            trigger: card,
            start: "top 86%",
          },
        }
      );
    });
  </script>

